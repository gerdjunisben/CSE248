<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>


<AnchorPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/18" fx:controller="Control.CollegeSearchController">
   <children>
      <Pane prefHeight="400.0" prefWidth="700.0">
         <children>
            <ListView fx:id="listArea" layoutX="250.0" layoutY="14.0" prefHeight="369.0" prefWidth="432.0" />
            <TextField fx:id="queryText" layoutX="101.0" layoutY="35.0" prefHeight="25.0" prefWidth="139.0" />
            <Button fx:id="addButton" layoutX="11.0" layoutY="70.0" mnemonicParsing="false" onAction="#addClick" prefHeight="25.0" prefWidth="229.0" text="add" />
            <ComboBox fx:id="attributeBox" layoutX="11.0" layoutY="35.0" prefHeight="25.0" prefWidth="83.0" />
            <TableView fx:id="searchTable" layoutX="26.0" layoutY="171.0" prefHeight="200.0" prefWidth="200.0">
              <columns>
                <TableColumn fx:id="attribute" prefWidth="75.0" text="Attribute" />
                <TableColumn fx:id="query" prefWidth="75.0" text="Query" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
            <Button fx:id="removeButton" layoutX="12.0" layoutY="109.0" mnemonicParsing="false" onAction="#removeClick" prefHeight="25.0" prefWidth="229.0" text="remove" />
         </children>
      </Pane>
   </children>
</AnchorPane>
